 <!-- Header Start -->
<div class="container-fluid header bg-white p-0">
    <div class="row g-0 align-items-center flex-column-reverse flex-md-row">
        <div class="col-md-6 p-5 mt-lg-5">
            <h1 class="display-5 animated fadeIn mb-4">@ViewBag.v0</h1>
            <nav aria-label="breadcrumb animated fadeIn">
                <ol class="breadcrumb text-uppercase">
                    <li class="breadcrumb-item"><a href="#">@ViewBag.v1</a></li>
                    <li class="breadcrumb-item"><a href="#">@ViewBag.v2</a></li>
                    <li class="breadcrumb-item text-body active" aria-current="page">@ViewBag.v3</li>
                </ol>
            </nav>
        </div>
        <div class="col-md-6 animated fadeIn">
            <img class="img-fluid" src="/real-estate-html-template/img/header.jpg" alt="">
        </div>
    </div>
</div>
<!-- Header End -->